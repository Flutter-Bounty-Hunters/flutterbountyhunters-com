        <a href="#WorkWithUs" class="inline-block mt-[48px] mx-auto px-[24px] py-[12px] text-[#FFFFFF] bg-[#4696E7] hover:bg-[#71B7FE] rounded-lg">Become a Funder</a>
      </div>
    </main>
    <footer></footer>

    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@4/dist/email.min.js"></script>
    <script type="text/javascript">
      (function(){
         emailjs.init({
           publicKey: "fOv5J8j1CnmUDzs_1",
           limitRate: {
            // Set the limit rate for the application
            id: 'app',
            // Allow 1 request per 10s
            throttle: 10000,
          },
         });
      })();

      var hireMeModal = document.getElementById("hireMeModal");
      hireMeModal.addEventListener("click", (e) => {
        closeHireMeModal();
      });

      var hireMePopup = document.getElementById("hireMePopup");
      hireMePopup.addEventListener("click", (e) => {
        console.log("stopClickEvent!: ", e);
        e.stopPropagation();
      });

      var hireMeForm = document.getElementById("hireMeForm");
      hireMeForm.addEventListener("submit", (e) => {
        e.preventDefault();
        submitHireMeModal();
      });

      function openClientModalForPartialFunding() {
        var select = document.getElementById("typeOfFunding");
        select.value = "partial-funder";

        openClientModal();
      }

      function openClientModalForPrimaryFunding() {
        var select = document.getElementById("typeOfFunding");
        select.value = "primary-funder";

        openClientModal();
      }

      function openClientModal() {
        var modal = document.getElementById("hireMeModal");
        modal.classList.remove("hidden");
      }

      function submitHireMeModal() {
        var hasError = false;

        var nameField = document.getElementById("name");
        var name = nameField.value;
        if (name.trim().length == 0) {
          nameField.classList.remove("border-slate-300");
          nameField.classList.add("border-red-500");
          hasError = true;
        } else {
          nameField.classList.remove("border-red-500");
          nameField.classList.add("border-slate-300");
        }

        var emailField = document.getElementById("email");
        var email = emailField.value;
        if (email.trim().length == 0) {
          emailField.classList.remove("border-slate-300");
          emailField.classList.add("border-red-500");
          hasError = true;
        } else {
          emailField.classList.remove("border-red-500");
          emailField.classList.add("border-slate-300");
        }

        var typeOfFundingField = document.getElementById("typeOfFunding");
        var typeOfFunding = typeOfFundingField.value;

        var messageField = document.getElementById("message");
        var message = messageField.value;
        if (message.trim().length == 0) {
          messageField.classList.remove("border-slate-300");
          messageField.classList.add("border-red-500");
          hasError = true;
        } else {
          messageField.classList.remove("border-red-500");
          messageField.classList.add("border-slate-300");
        }

        if (hasError === true) {
          // User needs to provide more info.
          return;
        }

        emailjs.send(
          "service_vcpx8t4",
          "template_fbh",
          {
            "reply_to": email,
            "from_name": name,
            "type_of_funding": typeOfFunding,
            "message": message,
          }
        ).catch((error) => {
          alert("Oops. Something went wrong when we tried to send your message. Please try again.\n\nIf it's still a problem then please visit me on Twitter/X @SuprDeclarative.");
        });

        closeHireMeModal();
      }

      function closeHireMeModal() {
        var modal = document.getElementById("hireMeModal");
        modal.classList.add("hidden");

        clearHireMeModal();
      }

      function clearHireMeModal() {
        var nameField = document.getElementById("name");
        nameField.value = "";

        var emailField = document.getElementById("email");
        emailField.value = "";

        var messageField = document.getElementById("message");
        messageField.value = "";
      }
    </script>
  </body>
</html>